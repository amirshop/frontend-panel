    "ag-grid-vue3": "^33.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "jspdf": "^2.5.2",
    "lodash": "^4.17.21",
    "pinia": "^2.3.1",
    "tinycolor2": "^1.6.0",
    "vue": "^3.5.13",
    "vue-advanced-cropper": "^2.8.9",
    "vue-i18n": "^11.1.1",
    "vue-request": "^2.0.4",
    "vue-router": "^4.5.0",
